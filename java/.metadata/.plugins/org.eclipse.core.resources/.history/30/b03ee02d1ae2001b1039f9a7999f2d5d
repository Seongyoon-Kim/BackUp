package java_standard.DecimalFormat;

public class DecimalFormatEx1 {
	
	public static void main(String[] args) {
		
		double number = 1234567.89;
		String[] pattern = {
				"0",
				"#",
				"0.0",
				"#.#",
				"0000000000.0000",
				"##########.####",
				"#.#-",
				"-#.#",
				"#,###.##",
				"#,####.##",
				"#E0",
				"0E0",
				"##E0",
				"0"
		}
		
	}

}
