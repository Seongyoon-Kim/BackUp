package com.test.question.q14;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Q004 {
	
	public static void main(String[] args) throws IOException {
		
		BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));
		
		for (int i=0; i<=10; i++) {
			
			System.out.print("파일명: ");
			String fileName = reader.readLine();
			
			int index = fileName.indexOf(".");
			
			if (fileName.substring(index + 1).equals("gif") && fileName.substring(index + 1).equals("jpg") && fileName.substring(index + 1).equals("png") && fileName.substring(index + 1).equals("hwp") && fileName.substring(index + 1).equals("doc")) {
				
				System.out.printf("gif: %d개", data);
				System.out.printf("jpg: %d개", data);
				System.out.printf("png: %d개", data);
				System.out.printf("hwp: %d개", data);
				System.out.printf("doc: %d개", data);
				
			}
			
		}
		
	}

}
